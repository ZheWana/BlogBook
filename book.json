{
    "title": "ZheWana-BlogBook",
    "description": "ZheWana`s blog books, use as knowledge trash bin.",
    "author": "ZheWana",
    "language": "zh-hans",
    "root": ".",
    "plugins": [
        "splitter",
        "flexible-alerts",
        "toggle-chapters",
        "-lunr",
        "-search",
        "search-pro",
        "codetabs"
    ],
    "pluginsConfig": {
        "flexible-alerts": {
            "note": {
                "label": {
                    "de": "Hinweis",
                    "en": "Note"
                }
            },
            "tip": {
                "label": {
                    "de": "Tipp",
                    "en": "Tip"
                }
            },
            "warning": {
                "label": {
                    "de": "Warnung",
                    "en": "Warning"
                }
            },
            "danger": {
                "label": {
                    "de": "Achtung",
                    "en": "Attention"
                }
            }
        }
    }
}